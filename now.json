{
	"version": 2,
	"alias": "portfolio.colinrumball.com",
	"env": {
		"CR_FRONTEND_ENDPOINT": "@cr_frontend_endpoint"
	},
	"builds": [
		{
			"src": "/public/**",
			"use": "@now/static"
		},
		{
			"src": "package.json",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "/public/(.*)",
			"dest": "/public/$1"
		}
	]
}
