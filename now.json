{
	"version": 2,
	"name": "Colin Rumball Portfolio Website",
	"alias": "portfolio.colinrumball.com",
	"builds": [
		{
			"src": "/public/**",
			"use": "@now/static"
		},
		{
			"src": "package.json",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "/public/(.*)",
			"dest": "/public/$1"
		}
	]
}
